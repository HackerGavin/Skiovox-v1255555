{
  "manifest_version": 2,
  "name": "Skiovox Helper",
  "version": "2.4.2",
  "description": "A helper for the Skiovox exploit",
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "history",
    "downloads",
    "tabs"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "commands": {
    "ACCESS_HISTORY": {
      "suggested_key": {
        "default": "Ctrl+H"
      },
      "description": "Open History"
    },
    "ACCESS_DOWNLOADS": {
      "suggested_key": {
        "default": "Ctrl+J"
      },
      "description": "Open Downloads"
    },
    "NEW_TAB": {
      "suggested_key": {
        "default": "Ctrl+T"
      },
      "description": "Open New Tab"
    },
    "CLOSE_TAB": {
      "suggested_key": {
        "default": "Ctrl+W"
      },
      "description": "Close Current Tab"
    },
    "RESTORE_TAB": {
      "suggested_key": {
        "default": "Ctrl+Shift+T"
      },
      "description": "Restore Last Closed Tab"
    },
    "NEW_WINDOW": {
      "suggested_key": {
        "default": "Ctrl+N"
      },
      "description": "Open New Window"
    },
    "NEW_INCOG_WINDOW": {
      "suggested_key": {
        "default": "Ctrl+Shift+N"
      },
      "description": "Open New Incognito Window"
    },
    "CLOSE_WINDOW": {
      "suggested_key": {
        "default": "Ctrl+Shift+W"
      },
      "description": "Close Current Window"
    },
    "TAB_NEXT": {
      "suggested_key": {
        "default": "Ctrl+Q"
      },
      "description": "Cycle Forward Through Tabs"
    }
  }
}
