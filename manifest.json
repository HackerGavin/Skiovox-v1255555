{
  "manifest_version": 2,
  "version": "2.4.3",
  "name": "Skiovox Helper",
  "description": "A helper for the Skiovox exploit",
  "icons": {
    "128": "icon.png"
  },
  "commands": {
    "NEW_TAB": {
      "suggested_key": {
        "default": "Ctrl+T"
      },
      "description": "Open a new tab"
    },
    "COPY": {
      "suggested_key": {
        "default": "Ctrl+Shift+C"
      },
      "description": "Copy selected text"
    },
    "PASTE": {
      "suggested_key": {
        "default": "Ctrl+Shift+V"
      },
      "description": "Paste clipboard contents"
    }
  },
  "background": {
    "scripts": [
      "/background/commands.js",
      "/background/setup.js",
      "/background/webstore-fix.js"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "/new-tab/main.html"
  },
  "permissions": [
    "commands",
    "tabs",
    "activeTab",
    "scripting",
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "/content-scripts/window-open-fix.js"
      ],
      "all_frames": true,
      "run_at": "document_start"
    }
  ]
}
